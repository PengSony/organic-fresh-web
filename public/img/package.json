  "scripts": {
    "postinstall": "npm run build",
    "debug": "node debug src/index.js",
    "build": "npm-run-all build:client build:server",
    "build:prod": "npm-run-all build:client:prod build:server",
    "build:server": "rm -r dist; babel ./src/server -s -D -d dist --ignore=node_modules/**/*,public/**/* --plugins=babel-plugin-root-import,babel-plugin-transform-runtime && cp -R src/server/public/ dist/public",
    "build:client": "./node_modules/.bin/webpack --config config/client/webpack.build.js  --info-verbosity verbose",
    "build:client:prod": "./node_modules/.bin/webpack --config config/client/webpack.prod.js",
    "start:server": "babel-node --presets=env src/server/index.js",
    "start:server:dev": "nodemon --ignore 'src/client/**/*' -e js,html,nj --exec 'node -r dotenv/config ./node_modules/.bin/babel-node' src/server/index.js",
    "start:client": "./node_modules/.bin/webpack --config config/client/webpack.dev.js --watch",
    "dev": "npm-run-all --parallel start:client start:server:dev",
    "start": "npm-run-all --parallel start:client start:server",
    "dist": "node dist/index.js",
    "test": "NODE_ENV=test node_modules/mocha/bin/_mocha --exit --compilers js:babel-core/register test/bootstrap.test.js test/unit/**/*.js",
    "test:cover": "PORT=1338 NODE_ENV=test nyc node_modules/mocha/bin/_mocha --exit --compilers js:babel-core/register test/bootstrap.test.js test/unit/*.js && cp coverage/coverage-final.json coverage/coverage.json",
    "test:acceptance": "NODE_ENV=test node_modules/mocha/bin/_mocha --exit --compilers js:babel-core/register test/bootstrap.test.js test/acceptance/",
    "test:acceptance:local": "NODE_ENV=local-test node_modules/mocha/bin/_mocha --exit --compilers js:babel-core/register test/bootstrap.test.js test/acceptance/",
    "lint:fix": "eslint --fix src",
    "lint": "eslint src",
    "lint:watch": "esw --watch --fix src",
    "precommit": "npm run lint && npm run test"
  },
